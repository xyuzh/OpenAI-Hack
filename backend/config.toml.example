# Example Configuration File for OpenAI-only Backend
# Copy this file to config.toml and update with your settings

[core]
# Basic application settings
debug = false
max_iterations = 30
max_budget_per_task = 10.0

[llm]
# OpenAI LLM Configuration
model = "gpt-4o"  # Options: gpt-4o, gpt-4o-mini, gpt-4-turbo, gpt-4, gpt-3.5-turbo
temperature = 0.0  # 0.0 for deterministic, up to 2.0 for creative
top_p = 1.0
max_output_tokens = 4096
num_retries = 4
timeout = 60

# Note: OPENAI_API_KEY should be set in .env file, not here

[agent]
# Agent configuration
enable_prompt_extensions = true
memory_enabled = true
max_input_tokens = 128000
max_output_tokens = 4096