  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "ephemeralStorage": {
    "sizeInGiB": 40
  },
  "cpu": "2048",
  "memory": "4096",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
