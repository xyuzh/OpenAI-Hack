          "valueFrom": "arn:aws:secretsmanager:us-west-1:251928524746:secret:usebase/dev-r67N6R:OPENAI_API_KEY::"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-region": "us-west-1",
          "awslogs-group": "/aws/ecs/usebase-agent-modem/dev",
          "awslogs-stream-prefix": "usebase-agent-modem",
          "mode": "non-blocking",
          "max-buffer-size": "25m"
        },
        "secretOptions": []
      }
